{
  "name": "rescape-geospatial-sankey",
  "version": "0.7.16",
  "description": "Visualize flow between nodes in a directed acyclic network on a Mapbox map",
  "keywords": [
    "d3",
    "d3-module",
    "sankey"
  ],
  "author": {
    "name": "Mike Bostock, forked by Andy Likuski",
    "url": "https://bost.ocks.org/mike/"
  },
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "homepage": "https://github.com/calocan/rescape-geospatial-sankey",
  "repository": {
    "type": "git",
    "url": "https://github.com/calocan/rescape-geospatial-sankey"
  },
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "test:watch": "yarn test -- --watch",
    "test:cov": "yarn test -- --coverage",
    "build:commonjs": "cross-env NODE_ENV=production BABEL_ENV=commonjs  node_modules/.bin/babel src --out-dir lib",
    "build:es": "BABEL_ENV=es NODE_ENV=es node node_modules/.bin/rollup  -c",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development node_modules/.bin/rollup -c -out-dir dist",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production NODE_ENV=production rollup -c -out-dir dist",
    "build": "yarn run build:commonjs && yarn run build:es",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn --new-version version patch && yarn publish",
    "publish-minor": "yarn --new-version version minor && yarn publish"
  },
  "dependencies": {
    "d3-array": "1",
    "d3-collection": "1",
    "d3-scale": "^1.0.7",
    "d3-shape": "^1.2.0",
    "rescape-helpers": "^1.1.78",
    "rescape-ramda": "^1.5.24"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.0",
    "babel-loader": "7.1.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.26.0",
    "babelrc-rollup": "^3.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-jest": "^21.17.0",
    "jest": "^23.4.0",
    "package-preamble": "0.1.0",
    "rollup": "^0.62.0",
    "rollup-plugin-auto-external": "^1.2.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-watch": "4.3.1",
    "tape": "4",
    "uglify-js": "3"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "ie >= 11"
            ]
          },
          "exclude": [
            "transform-async-to-generator"
          ],
          "modules": false
        }
      ]
    ],
    "sourceMaps": true,
    "plugins": [
      "transform-object-rest-spread"
    ],
    "env": {
      "commonjs": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      },
      "test": {
        "plugins": [
          [
            "transform-es2015-modules-commonjs-simple",
            {
              "noMangle": true
            }
          ],
          [
            "transform-runtime",
            {
              "polyfill": false,
              "regenerator": true
            }
          ]
        ],
        "presets": []
      }
    }
  }
}
