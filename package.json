{
  "name": "rescape-geospatial-sankey",
  "version": "0.7.20",
  "description": "Visualize flow between nodes in a directed acyclic network on a Mapbox map",
  "keywords": [
    "d3",
    "d3-module",
    "sankey"
  ],
  "author": {
    "name": "Mike Bostock, forked by Andy Likuski",
    "url": "https://bost.ocks.org/mike/"
  },
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "module": "esm/index.mjs",
  "files": [
    "esm",
    "src"
  ],
  "jsnext:main": "es/index.js",
  "homepage": "https://github.com/calocan/rescape-geospatial-sankey",
  "repository": {
    "type": "git",
    "url": "https://github.com/calocan/rescape-geospatial-sankey"
  },
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "test": "cross-env jest",
    "test:watch": "yarn test -- --watch",
    "test:cov": "yarn test -- --coverage",
    "build": "rollup -c",
    "prepare": "yarn run install-peers -f && cross-env ENABLE_INTEGRATION_TESTS=false yarn run clean && yarn run lint && yarn run build",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn version --patch && yarn publish --non-interactive",
    "publish-minor": "yarn version --minor && yarn publish --non-interactive"
  },
  "dependencies": {
    "d3-array": "2.2.0",
    "d3-collection": "1",
    "d3-scale": "^3.0.1",
    "d3-shape": "^1.2.0",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@types/jest": "^24.0.17",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babelrc-rollup": "^3.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.1.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-jest": "^22.15.1",
    "install-peers-cli": "^2.1.1",
    "jest": "^24.9.0",
    "package-preamble": "0.1.0",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "10.0.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1"
  },
  "peerDependencies": {
    "rescape-helpers": ">=1.1.99",
    "rescape-ramda": ">=1.5.78"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jestsetup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": "3x"
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-runtime",
          [
            "transform-es2015-modules-commonjs-simple",
            {
              "noMangle": true
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  }
}
